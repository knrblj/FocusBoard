{
  "manifest_version": 3,
  "name": "FocusBoard",
  "version": "1.0",
  "description": "Personal dashboard with birthday tracker, bookmarks, and sticky notes.",
  "permissions": ["storage", "alarms"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}